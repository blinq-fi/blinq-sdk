{
  "name": "blinq-sdk",
  "version": "0.1.2",
  "description": "SDK for all the things blinq",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src --dts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "blinq-sdk",
    "blinq-aggregator",
    "blinq-aggregator-sdk"
  ],
  "author": "mohan",
  "license": "MIT",
  "tsup": {
    "entry": ["src/index.ts"],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^2.2.3",
    "@types/node": "^22.13.10",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.6.1",
    "tslib": "^2.8.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
